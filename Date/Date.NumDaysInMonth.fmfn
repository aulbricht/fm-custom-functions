/**
 * 
 * Given a date, determine the number of days in that month
 *
 * @param {date} calendarDate - FileMaker formatted date
 *
 * @returnSuccess {number} count of days in the given month
 * 
 * @module Date
 * 
 * @version 1.0.0
 * @author Allen Ulbricht, https://github.com/aulbricht
 *
 */

Let ([

	~date = calendarDate ;

	~month = Month ( ~date );
	~year = Year ( ~date );

	~last = Date ( ~month + 1 ; 0 ; ~year );

~=""];

	Day ( ~last )

)
