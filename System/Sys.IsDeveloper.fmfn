/**
 *
 * Determine if the current user is the developer
 *
 * @returnSuccess {boolean} True if user is Full Access
 * 
 * @module System
 * 
 * @version 1.0.0
 * @author Allen Ulbricht, https://github.com/aulbricht
 *
 */


Get ( AccountPrivilegeSetName ) = "[Full Access]"
and Get ( AccountName ) = "developer"